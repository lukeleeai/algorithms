   "source": [
    "def permute(length, s):\n",
    "    if length == 1:\n",
    "        return s\n",
    "    return [\n",
    "        y+x\n",
    "        for y in permute(1, s)\n",
    "        for x in permute(length - 1, s)\n",
    "    ]"
   ]
  },
