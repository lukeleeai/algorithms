   "source": [
    "def beautifulArray(N):\n",
    "    res = [1]\n",
    "    while len(res) < N:\n",
    "        res = [i*2-1 for i in res] + [i*2 for i in res]\n",
    "    return [i for i in res if i<=N]"
   ]
  },
